#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Enter the number of terms: ";
    cin >> n;
    int a1= 0,a2= 1,a3;
    cout << "Fibonacci Series: ";
	for (int i = 1; i <= n; ++i) {
        cout << a1 << " ";
        a3 = a1+ a2;
        a1=a2;
        a2=a3;
    }
	 cout << endl;
	 return 0;
}
