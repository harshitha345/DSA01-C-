#include <iostream>
#include <string>
using namespace std;
int main() {
    string s1,s2;
	cout << "Enter the username: ";
    cin >> s1;
    cout << "Reenter the username: ";
    cin >> s2;
	if (s1==s2) {
        cout << "User name is Valid" << endl;
    } else {
        cout << "User name is Invalid" << endl;
    }
	return 0;
}
